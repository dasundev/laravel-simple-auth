{
    "name": "dasundev/laravel-simple-auth",
    "description": "A Simple method to create laravel authentication for an existing laravel project.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "auth",
        "php",
        "artisan"
    ],
    "authors": [
        {
            "name": "Dasun Tharanga",
            "email": "hello@dasun.dev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/ui": "^3.2"
    },
    "require-dev": {
        "laravel/framework": "^6.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dasundev\\LaravelSimpleAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasundev\\LaravelSimpleAuth\\LaravelSimpleAuthProvider"
            ]
        }
    },
    "prefer-stable": true
}
